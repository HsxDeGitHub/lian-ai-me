{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/apple-touch-icon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/dog.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets|.*\\..*|_next).*)",
      "destination": "/index.html"
    }
  ]
}
